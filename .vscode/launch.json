{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}\\PilkGame\\build\\Debug\\pilk-game.exe",
            "symbolSearchPath": "${workspaceRoot}\\PilkGame\\build\\Debug\\pilk-game.pdb",
            "preLaunchTask": "debug-build-win",
            "cwd": "${workspaceFolder}",
            "logging": {
                "moduleLoad": false,
                "trace": true
            },
            "visualizerFile": "${workspaceFolder}/my.natvis",
            "showDisplayString": true,
        },
    ]
}